<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.akif.projem.MainActivity">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/t_id"
        android:layout_alignLeft="@+id/t_id"
        android:layout_alignStart="@+id/t_id"
        android:layout_marginTop="27dp"
        android:id="@+id/t_username" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="121dp"
        android:layout_marginStart="121dp"
        android:layout_marginTop="42dp"
        android:id="@+id/t_id" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:id="@+id/t_surname"
        android:layout_below="@+id/t_name"
        android:layout_alignLeft="@+id/t_name"
        android:layout_alignStart="@+id/t_name" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="33dp"
        android:id="@+id/t_name"
        android:layout_below="@+id/t_username"
        android:layout_alignLeft="@+id/t_username"
        android:layout_alignStart="@+id/t_username" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/t_surname"
        android:layout_alignStart="@+id/t_surname"
        android:id="@+id/t_age" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/t_age"
        android:layout_alignRight="@+id/t_age"
        android:layout_alignEnd="@+id/t_age"
        android:layout_marginTop="33dp"
        android:id="@+id/t_tel" />

    <Button
        android:text="UserlarÄ± Goster"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:id="@+id/butonusergetir"
        android:layout_below="@+id/t_il"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="92dp"
        android:layout_marginStart="92dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:id="@+id/t_il"
        android:layout_below="@+id/t_tel"
        android:layout_toLeftOf="@+id/t_tel"
        android:layout_toStartOf="@+id/t_tel"
        android:layout_marginRight="17dp"
        android:layout_marginEnd="17dp" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/t_il"
        android:layout_toRightOf="@+id/t_tel"
        android:layout_toEndOf="@+id/t_tel"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp"
        android:id="@+id/t_ilce" />
</RelativeLayout>
